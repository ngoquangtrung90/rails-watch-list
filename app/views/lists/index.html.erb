<div class="navbar bg-light m-2">
  Lists Movie
  <%= link_to 'Create list', new_list_path %>
</div>
<%= cl_image_tag("zxrzuashjf7u1ea8pnyv", height: 500, width: 500, crop: :fit) %>
<div class="container">
  <% @lists.each do |list| %>
      <% if list.photo.attached? %>
        <%= cl_image_tag list.photo.key, height: 500, width: 500, crop: :fit %>
      <% end %>
      <h5 class="card-title"><p><%= link_to list.name, list_path(list) %></p></h5>
  <% end %>
</div>
